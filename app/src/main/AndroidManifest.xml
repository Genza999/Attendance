<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shamsa.myapplication">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="com.samsung.android.providers.context.permission.WRITE_USE_APP_FEATURE_SURVEY" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.Genza999.myapplication.Splash"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.Genza999.myapplication.AppBase" />
        <activity android:name="com.example.Genza999.myapplication.ReportActivity" />
        <activity
            android:name="com.example.Genza999.myapplication.profile_activity"
            android:parentActivityName="com.example.Genza999.myapplication.AppBase" />
        <activity
            android:name="com.example.Genza999.myapplication.Student_Registartion"
            android:parentActivityName="com.example.Genza999.myapplication.profile_activity" />
        <activity
            android:name="com.example.Genza999.myapplication.noteCreate"
            android:parentActivityName="com.example.Genza999.myapplication.noteActivity" />
        <activity
            android:name="com.example.Genza999.myapplication.attendanceActivity"
            android:parentActivityName="com.example.Genza999.myapplication.AppBase" />
        <activity
            android:name="com.example.Genza999.myapplication.Result_activity"
            android:parentActivityName="com.example.Genza999.myapplication.cgpa_activity" />
        <activity
            android:name="com.example.Genza999.myapplication.cgpa_activity"
            android:parentActivityName="com.example.Genza999.myapplication.AppBase" />
        <activity
            android:name="com.example.Genza999.myapplication.noteActivity"
            android:parentActivityName="com.example.Genza999.myapplication.AppBase" />
        <activity
            android:name="com.example.Genza999.myapplication.scheduler"
            android:parentActivityName="com.example.Genza999.myapplication.AppBase" />
        <activity
            android:name="com.example.Genza999.myapplication.make_schedule"
            android:parentActivityName="com.example.Genza999.myapplication.scheduler" />
        <activity
            android:name="com.example.Genza999.myapplication.SettingsActivity"
            android:label="@string/title_activity_settings"
            android:parentActivityName="com.example.Genza999.myapplication.AppBase" />
        <activity
            android:name="com.example.Genza999.myapplication.About"
            android:parentActivityName="com.example.Genza999.myapplication.AppBase" />
        <activity
            android:name="com.example.Genza999.myapplication.Edit_Student"
            android:parentActivityName="com.example.Genza999.myapplication.AppBase" />
    </application>

</manifest>